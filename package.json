{
  "name": "init-rn-env",
  "description": "Initialize react-native development environment",
  "version": "0.0.1",
  "maintainers": [
    {
      "name": "ielgnaw",
      "email": "wuji0223@gmail.com"
    }
  ],
  "dependencies": {
    "async": "^1.5.2",
    "chalk": "^1.1.3",
    "edp-core": "^1.0.32",
    "js-yaml": "^3.6.0",
    "less": "^2.7.1",
    "manis": "^0.3.0",
    "postcss": "^5.0.21",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-istanbul": "^0.8.0",
    "babel-node-debug": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "debug": "^2.2.0",
    "fecs": "^0.7.6",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "lint": "fecs lib test/**/*.spec.js --type=js",
    "compile": "./node_modules/.bin/babel src -d lib --source-maps inline --copy-files",
    "start": "npm run compile && node lib/index.js",
    "debug": "npm run compile && ./node_modules/.bin/babel-node-debug lib/index.js",
    "test": "npm run start && ./node_modules/.bin/_mocha --compilers js:babel-core/register --recursive",
    "coverage": "npm run start && ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha 'test/**/*.spec.@(js|es|es6)'",
    "coverage1": "npm run start && ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --recursive",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "sourcemap": "./node_modules/.bin/babel src -d lib -s",
    "watch": "./node_modules/.bin/babel -w src -d lib",
    "prepublish": "npm run compile"
  },
  "main": "./lib/checker.js",
  "bin": {
    "ire": "./bin/init-rn-env"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ielgnaw/init-rn-env.git"
  }
}
